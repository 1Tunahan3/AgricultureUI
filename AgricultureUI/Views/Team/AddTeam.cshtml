@model Entity.Concrete.Team
@{
    ViewData["Title"] = "AddTeam";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Add Team Member</h4>
            <form class="forms-sample" method="post">
                <div class="form-group">
                    <label for="exampleInputName1">Name Surname</label>
                    <input type="text" class="form-control" id="exampleInputName1" placeholder="" asp-for="PersonName">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail3">Title</label>
                    <input type="text" class="form-control" id="exampleInputEmail3" placeholder="" asp-for="Title">
                </div>

                <div class="form-group">
                    <label for="exampleInputEmail3">Image</label>
                    <input type="text" class="form-control" id="exampleInputEmail3" placeholder="" asp-for="ImageUrl">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail3">Facebook</label>
                    <input type="text" class="form-control" id="exampleInputEmail3" placeholder="" asp-for="FacebookUrl">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail3">Instagram</label>
                    <input type="text" class="form-control" id="exampleInputEmail3" placeholder="" asp-for="InstagramUrl">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail3">Twitter</label>
                    <input type="text" class="form-control" id="exampleInputEmail3" placeholder="" asp-for="TwitterUrl">
                </div>
                 <div class="form-group">
                    <label for="exampleInputEmail3">Web Site</label>
                    <input type="text" class="form-control" id="exampleInputEmail3" placeholder="" asp-for="WebsiteUrl">
                </div>
                
                <button type="submit" class="btn btn-primary me-2">Submit</button>
            </form>
        </div>
    </div>
</div>
